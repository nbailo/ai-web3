{
  "name": "aiweb3-api",
  "version": "0.1.0",
  "description": "NestJS API for Aqua maker/strategy orchestration",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "migration:generate": "NODE_NO_WARNINGS=1 dotenv -e .env -- typeorm-ts-node-commonjs -d src/typeorm.config.ts migration:generate",
    "migration:run": "NODE_NO_WARNINGS=1 ts-node --project tsconfig.json -r tsconfig-paths/register scripts/migrate.ts run",
    "migration:revert": "NODE_NO_WARNINGS=1 ts-node --project tsconfig.json -r tsconfig-paths/register scripts/migrate.ts revert",
    "migration:show": "NODE_NO_WARNINGS=1 ts-node --project tsconfig.json -r tsconfig-paths/register scripts/migrate.ts show"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "joi": "^18.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.3",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.9.3"
  }
}
